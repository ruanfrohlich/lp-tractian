.our-clients
  background-color $color-gray-900

  .wrapper
    padding-top 60px
    padding-bottom 60px

  .title
    margin-bottom 30px

  .clients
    display flex
    align-items baseline
    justify-content space-between
    column-gap 10px 
    
    &__item
      width 100%
      max-width 150px
      height clamp(40px, 5vw, 85px)

      img  
        width 100%
        height 100%
        object-fit contain
        image-rendering optimizeQuality